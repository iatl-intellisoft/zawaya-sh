<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_employee" model="ir.sequence">
            <field name="name">hr.employee</field>
            <field name="code">hr.employee</field>
            <field name="prefix">EMP/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_contract" model="ir.sequence">
            <field name="name">hr.contract</field>
            <field name="code">hr.contract</field>
            <field name="prefix"> CO/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>